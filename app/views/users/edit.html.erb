<h1>アカウント情報編集</h1>

<%= form_with(model:@user, local: true) do |f| %>

  <%= f.label :name, "名前" %>
  <%= f.text_field :name %>

  <%= f.label :email, "メールアドレス" %>
  <%= f.text_field :email %>

  <%#= f.label :password, "パスワード" %>
  <%#= f.password_field :password %>

  <%#= f.label :password_confirmation, "パスワード（確認）" %>
  <%#= f.password_field :password_confirmation %>

  <%= f.submit "更新" %>
<% end %>
